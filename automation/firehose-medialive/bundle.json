{
   "name": "firehose-medialive",
  "base_url": "https://github.com/hydrolix/integration-deployment-templates/blob/main/automation/firehose-medialive",
  "beta": true,
  "method": "firehose",
  "source": "medialive",
  "dashboard": {
    "path":"dashboards/current.json",
    "project_var": "__PROJECT_NAME__"
  },
  "tables": [
    {
      "dashboard_var": "__ENCODING_TABLE__",
      "name": "medialive_encoding",
      "transforms": [
        {
          "data_template_path": "transformations/encoding/sample_data_template.json",
          "path": "transformations/encoding/current.json"
        }
      ]
    },
    {
      "dashboard_var": "__METRICS_TABLE__",
      "name": "medialive_metrics",
      "transforms": [
        {
          "data_template_path": "transformations/metrics/sample_data_template.json",
          "path": "transformations/metrics/current.json"
        }
      ]
    }
  ],
  "ui": {
    "primary_url": "https://cascade-marketplace.s3.us-east-1.amazonaws.com/public/assets/docs/ElementalMediaLive.html",
    "method": {
      "full_title": "Amazon Data Firehose",
      "icon_url": "https://cascade-marketplace.s3.us-east-1.amazonaws.com/public/assets/icons/firehose.png"
    },
    "source": {
      "full_title": "AWS Elemental MediaLive",
      "icon_url": "https://cascade-marketplace.s3.us-east-1.amazonaws.com/public/assets/icons/medialive.png"
    },  
    "data_category": "video"
  },
   "metadata": {
    "version": "1.0.0",
    "maintainer": "jgoodson@hydrolix.io",
    "description": "Amazon Firehose MediaLive Itegration",
    "channel_type": "AWS"
  }
}

